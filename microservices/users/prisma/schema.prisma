generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model tb_rwms_users {
  id       Int      @id @default(autoincrement())
  name     String
  username String
  email    String   @unique
  password String
  celular  String
  active   Boolean  @default(false)
  role     String   @default("user")
}
